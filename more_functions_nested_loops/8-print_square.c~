#include "main.h"
/**
 * print_square - check the code
 *@size: siz of square
 *
 * Return: Always 0.
 */
void print_square(int size)
{
  int s = size;
  if (size < 1)
    _putchar('\n');
  while (s > 0)
    {
    while (size > 0)
      {
	 _putchar('#');
	 size--;
      }
    _putchar('\n');
    s--;
    }

}
